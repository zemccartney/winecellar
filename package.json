{
  "name": "balliswine",
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "astro": "astro",
    "build": "astro check && astro build",
    "cf:wip": "git commit -m \"[CF-Pages-Skip] WIP\" && git push",
    "check": "astro check && npm run fmt:check && npm run lint",
    "deploy": "astro build && wrangler deploy",
    "deps": "ncu -i --format group --cooldown 3",
    "fmt": "prettier . --write",
    "fmt:check": "prettier . --check",
    "info": "astro info",
    "lint": "eslint .",
    "lint:debug": "eslint --inspect-config",
    "preview": "astro preview",
    "start": "concurrently --kill-others --names astro,ts -c red,blue \"astro dev\" \"npm run typewatch\"",
    "typewatch": "astro check --watch --preserveWatchOutput"
  },
  "dependencies": {
    "@astrojs/mdx": "^4.3.7",
    "@fontsource-variable/noto-emoji": "^5.2.11",
    "@fontsource/geist-mono": "^5.2.7",
    "@fontsource/geist-sans": "^5.2.5",
    "@fontsource/permanent-marker": "^5.2.7",
    "@tailwindcss/vite": "^4.1.14",
    "astro": "^5.14.1",
    "clsx": "^2.1.1",
    "leaflet": "^1.9.4",
    "leaflet.markercluster": "^1.5.3",
    "lite-youtube-embed": "^0.3.3",
    "tailwindcss": "^4.1.14"
  },
  "devDependencies": {
    "@astrojs/check": "^0.9.4",
    "@eslint/compat": "^1.4.0",
    "@eslint/js": "^9.37.0",
    "@eslint/json": "^0.13.2",
    "@types/leaflet": "^1.9.21",
    "@types/leaflet.markercluster": "^1.5.6",
    "@typescript-eslint/parser": "^8.46.1",
    "concurrently": "^9.2.1",
    "eslint": "^9.37.0",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-astro": "^1.3.1",
    "eslint-plugin-jsx-a11y": "^6.10.2",
    "eslint-plugin-package-json": "^0.57.0",
    "eslint-plugin-perfectionist": "^4.15.1",
    "eslint-plugin-unicorn": "^61.0.2",
    "globals": "^16.4.0",
    "npm-check-updates": "^19.1.1",
    "prettier": "^3.6.2",
    "prettier-plugin-astro": "^0.14.1",
    "prettier-plugin-tailwindcss": "^0.6.14",
    "typescript": "^5.9.3",
    "typescript-eslint": "^8.46.1",
    "wrangler": "^4.43.0"
  },
  "engines": {
    "node": ">=22 <23"
  },
  "engineStrict": true
}
